[
  {
    "format": "yyyy-MM-dd",
    "key": "pubdate",
    "required": true,
    "templateUrl": "/static/templates/weko_deposit/datepicker.html",
    "title": "PubDate",
    "title_i18n": {
      "en": "PubDate",
      "ja": "\u516c\u958b\u65e5"
    },
    "type": "template"
  },
  {
  "add": "New",
  "items": [
    {
      "key": "parentkey[].filename",
      "title": "表示名",
      "titleMap": [],
      "title_i18n": {
        "en": "FileName",
        "ja": "表示名"
      },
      "type": "select"
    },
    {
      "key": "parentkey[].displaytype",
      "title": "表示形式",
      "titleMap": [
        {
          "name": "詳細表示",
          "value": "detail"
        },
        {
          "name": "簡易表示",
          "value": "simple"
        },
        {
          "name": "プレビュー",
          "value": "preview"
        }
      ],
      "title_i18n": {
        "en": "Preview",
        "ja": "表示形式"
      },
      "type": "select"
    },
    {
      "key": "parentkey[].licensetype",
      "title": "ライセンス",
      "titleMap": [
        {
          "name": "自由入力",
          "value": "license_free"
        },
        {
          "name": "Creative Commons : 表示",
          "value": "license_0"
        },
        {
          "name": "Creative Commons : 表示 - 継承",
          "value": "license_1"
        },
        {
          "name": "Creative Commons : 表示 - 改変禁止",
          "value": "license_2"
        },
        {
          "name": "Creative Commons : 表示 - 非営利",
          "value": "license_3"
        },
        {
          "name": "Creative Commons : 表示 - 非営利 - 継承",
          "value": "license_4"
        },
        {
          "name": "Creative Commons : 表示 - 非営利 - 改変禁止",
          "value": "license_5"
        }
      ],
      "title_i18n": {
        "en": "License",
        "ja": "ライセンス"
      },
      "type": "select"
    },
    {
      "condition": "model.parentkey[arrayIndex].licensetype == 'license_free'",
      "key": "parentkey[].licensefree",
      "notitle": true,
      "type": "textarea"
    },
    {
      "template": "<div class='text-center'><a class='btn btn-primary' href='/ezas/pdf-detect-weko.html' target='_blank' role='button'>{{ form.title }}</a></div>",
      "title": "剽窃チェック",
      "title_i18n": {
        "en": "Check Plagiarism",
        "ja": "剽窃チェック"
      },
      "type": "template"
    },
    {
      "default": "open_restricted",
      "key": "parentkey[].accessrole",
      "title": "アクセス",
      "titleMap": [
        {
          "name": "オープンアクセス",
          "value": "open_access",
          "name_i18n": {
            "en": "Open Access",
            "ja": "オープンアクセス"
          }
        },
        {
          "name": "オープンアクセス日を指定する",
          "value": "open_date",
          "name_i18n": {
            "en": "Input Open Access Date",
            "ja": "オープンアクセス日を指定する"
          }
        },
        {
          "name": "ログインユーザのみ",
          "value": "open_login",
          "name_i18n": {
            "en": "Registered User Only",
            "ja": "ログインユーザのみ"
          }
        },
        {
          "name": "公開しない",
          "value": "open_no",
          "name_i18n": {
            "en": "Do Not Publish",
            "ja": "公開しない"
          }
        },
        {
          "name": "限定公開",
          "value": "open_restricted",
          "name_i18n": {
            "en": "Limited Access",
            "ja": "限定公開"
          }
        }
      ],
      "title_i18n": {
        "en": "Access",
        "ja": "アクセス"
      },
      "type": "radios"
    },
    {
      "condition": "model.parentkey[arrayIndex].accessrole == 'open_date'",
      "format": "yyyy-MM-dd",
      "key": "parentkey[].accessdate",
      "templateUrl": "/static/templates/weko_deposit/datepicker.html",
      "title": "公開日",
      "title_i18n": {
        "en": "Opendate",
        "ja": "公開日"
      },
      "type": "template"
    },
    {
      "condition": "model.parentkey[arrayIndex].accessrole == 'open_date' || model.parentkey[arrayIndex].accessrole == 'open_login'",
      "key": "parentkey[].groups",
      "title": "グループ",
      "titleMap": [],
      "title_i18n": {
        "en": "Group",
        "ja": "グループ"
      },
      "type": "text"
    }
  ],
  "key": "parentkey",
  "style": {
    "add": "btn-success"
  },
  "title_i18n": {
    "en": "File",
    "ja": "ファイル"
  }
}
]
